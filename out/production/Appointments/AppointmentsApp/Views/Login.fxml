<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="AppointmentsApp.Views.LoginController">
   <children>
      <VBox layoutX="92.0" layoutY="135.0" prefHeight="178.0" prefWidth="416.0">
         <children>
            <Label fx:id="login_countryLabel" text="Country" />
            <TextField fx:id="login_countryField" disable="true" editable="false" />
            <Label fx:id="login_usernameLabel" text="Username">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="login_usernameField" onKeyReleased="#handleEnter" text="test" />
            <Label fx:id="login_passwordLabel" text="Password">
               <VBox.margin>
                  <Insets top="12.0" />
               </VBox.margin>
            </Label>
            <PasswordField fx:id="login_passwordField" onKeyReleased="#handleEnter" text="test" />
            <Button fx:id="login_button" mnemonicParsing="false" onAction="#login" text="Login">
               <VBox.margin>
                  <Insets top="12.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
      <Label alignment="CENTER" layoutX="92.0" layoutY="80.0" prefHeight="40.0" prefWidth="416.0" text="C195: Appointments App">
         <font>
            <Font size="28.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
